
<%- include("header")  -%>

<div class="box" id="heading">
    <h1><%= listTitle %> </h1>  <!--printing date-->
</div>

<div class="box">
    <%  for(var i=0 ; i<newItems.length; i++){ %>  <!--printing Todo lists value-->
    <div class="item">
        <input type="checkbox">
        <p> <%= newItems[i]    %></p>
    </div>
    <% } %>



    <form class="item" action="/" method="POST">

        <input type="text" name="newItem" placeholder="Add New Items" autocomplete="off" required>
     
        <button type="submit" name="list" value=<%= listTitle %>> +</button>
    </form>
</div>
    

<%- include("footer")  -%>

     


    
    
